{
  "dirs_config": {
    "onepanel": {
      "input_dir": "rel/onepanel",
      "target_dir": "rel/test_cluster"
    }
  },
  "os_configs": {
    "cfg1": {
      "storages": [
        {
          "type": "posix",
          "name": "/mnt/st1"
        },
        {
          "type": "posix",
          "name": "/mnt/st2"
        },
        {
          "type": "ceph",
          "name": "someCeph",
          "pool": "onedata:8"
        },
        {
          "type": "s3",
          "name": "someS3",
          "bucket": "onedata",
          "disable_iam_mock": false
        }
      ]
    }
  },
  "onepanel_domains": {
    "onezone": {
      "image": "docker.onedata.org/onepanel:onezone",
      "onepanel": {
        "node1": {
          "vm.args": {
            "setcookie": "test_cookie"
          },
          "sys.config": {
            "onepanel": {
              "release": "onezone"
            }
          }
        },
        "node2": {
          "vm.args": {
            "setcookie": "test_cookie"
          },
          "sys.config": {
            "onepanel": {
              "release": "onezone"
            }
          }
        }
      }
    },
    "oneprovider": {
      "image": "docker.onedata.org/onepanel:oneprovider",
      "os_config": "cfg1",
      "onepanel": {
        "node1": {
          "vm.args": {
            "setcookie": "test_cookie"
          },
          "sys.config": {
            "ctool": {
              "verify_oz_cert": false
            },
            "onepanel": {
              "advertise_address": {
                "string": "239.255.1.1"
              }
            }
          }
        },
        "node2": {
          "vm.args": {
            "setcookie": "test_cookie"
          },
          "sys.config": {
            "ctool": {
              "verify_oz_cert": false
            },
            "onepanel": {
              "advertise_address": {
                "string": "239.255.1.1"
              }
            }
          }
        }
      }
    }
  }
}
