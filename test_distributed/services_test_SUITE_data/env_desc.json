{
  "dirs_config": {
    "onepanel": {
      "input_dir": "_build/default/rel/onepanel",
      "target_dir": "_build/default/rel/test_cluster"
    }
  },
  "os_configs": {
    "cfg1": {
      "storages": [
        {
          "type": "posix",
          "name": "/mnt/st1",
          "storagePathType": "canonical"
        },
        {
          "type": "posix",
          "name": "/mnt/st2",
          "storagePathType": "canonical"
        },
        {
          "type": "ceph",
          "name": "someCeph",
          "pool": "onedata:8",
          "storagePathType": "flat"
        },
        {
          "type": "cephrados",
          "name": "someCephRados",
          "pool": "onedata:8",
          "storagePathType": "flat"
        },
        {
          "type": "s3",
          "name": "someS3",
          "bucket": "onedata",
          "storagePathType": "flat",
          "disable_iam_mock": false
        },
        {
          "type": "swift",
          "name": "someSwift",
          "container": "swift",
          "storagePathType": "flat"
        },
        {
          "type": "glusterfs",
          "name": "someGlusterfs",
          "volume": "data",
          "transport": "tcp",
          "mountpoint": "/",
          "xlatorOptions": "",
          "storagePathType": "canonical"
        },
        {
          "type": "webdav",
          "name": "someWebDAV",
          "storagePathType": "canonical"
        },
        {
          "type": "xrootd",
          "name": "someXRootD",
          "storagePathType": "canonical"
        }

      ]
    }
  },
  "onepanel_domains": {
    "onezone": {
      "image": "docker.onedata.org/onezone-dev:VFS-6841-add-time-diff-measuring-and-switch-to-clock-everywhere", "fixme": "revert",
      "onepanel": {
        "node1": {
          "vm.args": {
            "setcookie": "test_cookie"
          },
          "sys.config": {
            "onepanel": {
              "cluster_type": "onezone",
              "clock_synchronization_interval_seconds": 5
            }
          }
        },
        "node2": {
          "vm.args": {
            "setcookie": "test_cookie"
          },
          "sys.config": {
            "onepanel": {
              "cluster_type": "onezone",
              "clock_synchronization_interval_seconds": 5
            }
          }
        }
      }
    },
    "oneprovider": {
      "os_config": "cfg1",
      "image": "docker.onedata.org/oneprovider-dev:VFS-6841-add-time-diff-measuring-and-switch-to-clock-everywhere", "fixme": "revert",
      "onepanel": {
        "node1": {
          "vm.args": {
            "setcookie": "test_cookie"
          },
          "sys.config": {
            "onepanel": {
              "clock_synchronization_interval_seconds": 5
            }
          }
        },
        "node2": {
          "vm.args": {
            "setcookie": "test_cookie"
          },
          "sys.config": {
            "onepanel": {
              "clock_synchronization_interval_seconds": 5
            }
          }
        }
      }
    }
  }
}
