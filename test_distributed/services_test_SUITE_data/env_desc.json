{
  "dirs_config": {
    "onepanel": {
      "input_dir": "rel/onepanel",
      "target_dir": "rel/test_cluster"
    }
  },
  "onepanel_domains": {
    "onezone": {
      "image": "docker.onedata.org/onepanel:onezone",
      "onepanel": {
        "node1": {
          "vm.args": {
            "setcookie": "test_cookie"
          },
          "sys.config": {
            "onepanel": {}
          }
        },
        "node2": {
          "vm.args": {
            "setcookie": "test_cookie"
          },
          "sys.config": {
            "onepanel": {}
          }
        }
      }
    },
    "oneprovider": {
      "image": "docker.onedata.org/onepanel:oneprovider",
      "onepanel": {
        "node1": {
          "vm.args": {
            "setcookie": "test_cookie"
          },
          "sys.config": {
            "ctool": {
              "verify_oz_cert": false
            },
            "onepanel": {
              "advertise_address": {
                "string": "239.255.1.1"
              }
            }
          }
        },
        "node2": {
          "vm.args": {
            "setcookie": "test_cookie"
          },
          "sys.config": {
            "ctool": {
              "verify_oz_cert": false
            },
            "onepanel": {
              "advertise_address": {
                "string": "239.255.1.1"
              }
            }
          }
        }
      }
    }
  }
}
